{
  "name": "images-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "delete-dir": "rm -rf dist",
    "write-props": "node PropertiesParser.js",
    "create-props": "npm run delete-dir && npm run write-props",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon src/server.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "start:dev": "npm run build && npm run watch",
    "start:prod": "nodemon dist/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "body-parser": "^1.19.1",
    "cloudinary": "^1.28.1",
    "cors": "^2.8.5",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.8",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
